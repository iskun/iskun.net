<div class="col-lg-12" style="padding: 0px" ng-show="isAdding==true" ng-cloak>
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>Thêm tài nguyên</h5>
            <div class="ibox-tools">
                <a ng-click="closeAddResource()" onclick="return false">
                    <i class="fa fa-times"></i>
                </a>
            </div>
        </div>
        <div class="ibox-content">
            <div class="row">
                <div class="form-group col-lg-4">
                    <label class="col-sm-12 control-label">Môn học</label>
                    <div class="col-sm-12">
                        <select name="account" class="form-control " ng-model="newResource.filter.subjects" ng-options="item as item.name for item in teachingsubjects track by item.id"></select> 
                        <small class="text-danger">[[result.errors.filter.subjects]]</small>
                    </div> 
                </div>
                <div class="form-group col-lg-4">
                    <label class="col-sm-12 control-label">Chương</label>
                    <div class="col-sm-12">
                        <select  name="account" class="form-control" ng-model="newResource.filter.subjectschapters" ng-options="item as item.name for item in newResource.filter.subjects.subjectschapters track by item.id"></select> 
                        <small class="text-danger">[[result.errors.filter.subjectschapters]]</small>
                    </div>
                </div>
                <div class="form-group col-lg-4">
                    <label class="col-sm-12 control-label">Chia sẻ</label>
                    <div class="col-sm-12">
                        <select  name="account" class="form-control " ng-model="newResource.filter.sharings" ng-options="item as item.name for item in sharings track by item.id"></select> 
                        <small class="text-danger">[[result.errors.filter.sharings]]</small>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-lg-12">
                    <div class="col-sm-12">
                        <input file-model = "file" type="file" id="addFile" style="display: none">
                        <input type="text" placeholder="Tên tài nguyên" name="account" class="form-control" ng-model="newResource.title"> 
                        <small class="text-danger">[[result.errors.title]]</small>
                    </div>
                    <div class="col-sm-12">
                        <textarea class="form-control" placeholder="Mô tả về tài nguyên"  ng-model="newResource.content"></textarea>
                        <small class="text-danger">[[result.errors.content]]</small>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <small class="text-danger">[[result.errors.files]]</small>
                </div>
                <div ng-repeat="file in uploadingFiles" class="col-lg-3" style="padding: 0px;">
                    <div class="ibox float-e-margins" style="padding: 0px 5px 5px 5px;">
                        <div class="ibox-content" style="padding: 0px; border:1px solid #e7eaec;">
                            <div class="carousel ">
                                <div class="carousel-inner">
                                    <div class="item active" style="padding-bottom: 100%; overflow: hidden; position: relative">
                                        <img alt="image" ng-if="file.src && !file.file.thumbnail" class="img-responsive" style="position: absolute;top:0; bottom: 0px; left: 0px; right: 0px; margin: auto" ng-src="[[file.src]]">
                                        <img alt="image" ng-if="file.file.thumbnail" class="img-responsive" style="position: absolute;top:0; bottom: 0px; left: 0px; right: 0px; margin: auto" ng-src="thumbnails/[[file.file.thumbnail]]">
                                        <div ng-hide="file.src" style="position: absolute;top:0; bottom: 0px; left: 0px; right: 0px; margin: auto; padding-top: 50%">
                                            <div class="sk-spinner sk-spinner-three-bounce">
                                                <div class="sk-bounce1"></div> 
                                                <div class="sk-bounce2"></div>
                                                <div class="sk-bounce3"></div>
                                            </div>
                                        </div>
                                        <div class="carousel-caption" style="top:50%; margin-top: 10px;">
                                            <div ng-hide="file.file" class="progress progress-mini">
                                                <div class="progress-bar" style="width: [[file.percent]]%;"></div>
                                            </div>
                                            <div ng-show="file.file" >
                                                <i class="fa fa-check btn-primary"></i>
                                            </div>
                                        </div>

                                    </div>
                                    <div style="padding: 5px;">
                                        <p>[[file.name]]</p>
                                        <input placeholder="Mô tả tệp" type="text" class="form-control" ng-model="file.file.description">
                                        <label>Phân loại</label>  
                                        <div ng-repeat="category in filescategories" ><label style="font-size: 12px;"> <input icheck type="radio" value="[[category.id]]" ng-model="file.file.filescategories.id"  name="[[file.file.id]]"> <i></i> [[category.name]] </label></div>
                                    </div>
                                </div> 

                            </div>
                        </div>
                    </div>
                </div> 

            </div>
            <div class="row">
                <div class="col-sm-4 pull-left">
                    <button ng-click="addFile()" class="btn  btn-lg btn-block btn-default fa fa-plus"> Thêm tệp </button>
                </div> 
                <div class="col-sm-4"></div>
                <div class="col-sm-4 pull-right">
                    <button ng-click="addResource()" class="btn  btn-lg btn-block btn-primary  fa fa-save"> Thêm tài nguyên </button>
                </div> 
            </div>
        </div>
    </div>
</div>

<div id="allResources">
    <div ng-repeat="resource in resources" id="resource_[[$index]]" ng-init="rootIndex = $index" ng-cloak class="col-lg-3 container-fluid" style="padding: 0px;">

        <div class="ibox float-e-margins" style="padding: 0px 5px 5px 5px;">
            <div class="ibox-content" style="padding: 0px; border:1px solid #e7eaec;">
                <div class="carousel slide" data-ride="carousel" data-interval="3000" id="carousel[[resource.id]]" >
                    <ol style="bottom: 10px; z-index: 16;" ng-if="resource.postsfiles.length > 1"   class="carousel-indicators" >
                        <li ng-repeat="f in resource.postsfiles" data-slide-to="[[$index]]" data-target="#carousel[[resource.id]]" ng-class="$index==0 ? 'active' : ''" class=""></li>
                    </ol>
                    <div ng-click="viewResource(resource,rootIndex)" ng-if="resource.postsfiles.length > 1" class="carousel-indicators" style="cursor: pointer; top:0px; left:0%; right:0%; text-align: right; width: 100%; padding: 0px; margin-left: 0px;">
                        <p><span class="label label-info">[[resource.postsfiles.length]] tệp</span></p>
                        <p ng-repeat="extension in resource.extensions"><span class="label label-default" >.[[extension.extension]] ([[extension.numbers]])</span></p> 
                    </div>
                    <div class="carousel-inner">
                        <div ng-repeat="f in resource.postsfiles" ng-class="$index==0 ? 'active' : ''" class="item">
                            <img ng-if="f.files.thumbnail.length>0"  ng-click="viewResource(resource,rootIndex)" alt="image" class="img-responsive" style="width: 100%; cursor: pointer" ng-src="/thumbnails/[[f.files.thumbnail]]">
                            <img ng-if="f.files.thumbnail.length==0"  ng-click="viewResource(resource,rootIndex)" alt="image" class="img-responsive" style="width: 100%; cursor: pointer" src="http://www.bhphotovideo.com/images/images2500x2500/fujifilm_16470245_x_t10_mirrorless_digital_camera_1149205.jpg">
                            <div class="carousel-caption" style="background-color: rgba(0,0,0,0.3);padding-bottom: 0px; bottom: 0px; width: 100%; left: 0%; right:0%">
                                <p class="">[[f.files.filename | limitTo:30 ]] <span ng-if="f.files.filename.length>30">...</span></p>  
                            </div> 
                            <div ng-click="viewResource(resource,rootIndex)" class="carousel-caption" style="top:0px; left:0%; right:0%; text-align: left; cursor: pointer">
                                <p><span class="label label-info">.[[f.files.extension]]</span></p> 
                            </div>
                        </div>
                    </div> 
                    <a style="z-index: 16;" ng-if="resource.postsfiles.length>1"  data-slide="prev" href="#carousel[[resource.id]]"   class="left carousel-control">
                        <span class="icon-prev"></span>
                    </a> 
                    <a style="z-index: 16;" ng-if="resource.postsfiles.length>1"  data-slide="next" href="#carousel[[resource.id]]"   class="right carousel-control">
                        <span class="icon-next"></span>
                    </a>
                </div>

                <div class="center-version" style="padding: 0px 0px 0px 0px; height: 60px; ">
                    <div class="vote-item" style="padding: 0px 5px 0px 5px">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="vote-actions" style="margin-right: 0px;">
                                    <a href="#">
                                        <i class="fa fa-chevron-up"> </i>
                                    </a>
                                    <div>[[resource.likesnumbers]]</div>
                                    <a href="#">
                                        <i class="fa fa-chevron-down"> </i>
                                    </a>
                                </div>  
                                <a title="[[resource.title]]" ng-click="viewResource(resource,$index)" class="vote-title" onclick="return false" href="#" style="font-size: 12px; margin-left: 30px; height: 30px;">
                                    [[resource.title | limitTo: 60]] <span ng-show="resource.title.length>60">...</span>
                                </a>
                                <div class="vote-info" style="margin-left: 30px;"> 
                                    <i class="fa fa-comments-o"></i> <a href="#"> ([[resource.commentsnumbers]])</a>
                                    <i class="fa fa-user"></i> <a target="_blank" href="/[[resource.users.slugs.slug]]">[[resource.users.fullname]]</a>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div> 
</div>
